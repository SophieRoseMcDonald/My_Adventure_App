<div class="adventure_index">

 <% @adventure.each do |adventure| %>
 <a href="/adventures/<%=adventure.id%>">
   <p> <%= adventure.name %> </p> </a>
   <img class="adventure_image" src="<%= adventure.image_url %>" alt="">
 <% end %>
 
</div>
<div class="tag_index">

 <% @tag.each do |tag| %>
   <p> <%= tag.tag_name %> </p>


 <% end %>
</div>
